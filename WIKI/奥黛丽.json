{
    "奥黛丽语音-卡拉彼丘CN.mp3": {
        "text": "卡拉彼丘",
        "tag": "卡拉彼丘",
        "_id": 2530
    },
    "奥黛丽语音-卡拉彼丘JP.mp3": {
        "text": "カラビヤウ",
        "tag": "卡拉彼丘",
        "_id": 2531
    },
    "奥黛丽语音-001CN.mp3": {
        "text": "要是以为这样就能接近我，那就大错特错了！不过，你足够有诚意的话，还是可以考虑的……",
        "tag": "默认交谈",
        "_id": 2532
    },
    "奥黛丽语音-001JP.mp3": {
        "text": "この程度で私に近づけたと思わないことねま、もう?もっと誠意を見せてくれれば考えてあげなくもないよ。",
        "tag": "默认交谈",
        "_id": 2533
    },
    "奥黛丽语音-002CN.mp3": {
        "text": "你想要跟我合作？我挑选商业伙伴可是很严格的哦。",
        "tag": "默认交谈",
        "_id": 2534
    },
    "奥黛丽语音-002JP.mp3": {
        "text": "私と手を組みたいの?ふーん、大丈夫かしら?ビジネスパートナーはかなり厳選してるわよ。",
        "tag": "默认交谈",
        "_id": 2535
    },
    "奥黛丽语音-003CN.mp3": {
        "text": "哼，我就知道是你，这样有失礼节哦！",
        "tag": "默认交谈",
        "_id": 2536
    },
    "奥黛丽语音-003JP.mp3": {
        "text": "ふっ、やっぱあんただったわね。ほんっと、礼儀を知らないんだから。",
        "tag": "默认交谈",
        "_id": 2537
    },
    "奥黛丽语音-004CN.mp3": {
        "text": "明白吗！我可不会轻易改变立场哦。",
        "tag": "默认交谈",
        "_id": 2538
    },
    "奥黛丽语音-004JP.mp3": {
        "text": "私は簡単に立場を変えたりしないの。おわかり?",
        "tag": "默认交谈",
        "_id": 2539
    },
    "奥黛丽语音-005CN.mp3": {
        "text": "快去查收一下新邮件吧。如果不是你，我才不会特意提醒呢！",
        "tag": "默认交谈",
        "_id": 2540
    },
    "奥黛丽语音-005JP.mp3": {
        "text": "新着メールの確認をしなさい。あんただからアドバイスしてるんだからね。",
        "tag": "默认交谈",
        "_id": 2541
    },
    "奥黛丽语音-问候-早CN.mp3": {
        "text": "哈啊，看你这睡眼惺忪的样子，倒是有那么点儿可爱。",
        "tag": "问候-早",
        "_id": 2542
    },
    "奥黛丽语音-问候-早JP.mp3": {
        "text": "ふうん。アンタの寝ぼけてるところ、わりと可愛いのね。",
        "tag": "问候-早",
        "_id": 2543
    },
    "奥黛丽语音-问候-晚CN.mp3": {
        "text": "晚上好，你还是和往常一样，一脸悠闲的样子呢。",
        "tag": "问候-晚",
        "_id": 2544
    },
    "奥黛丽语音-问候-晚JP.mp3": {
        "text": "こんばんは。なーんか相変わらずのんきな顔してるわねえ。",
        "tag": "问候-晚",
        "_id": 2545
    },
    "奥黛丽语音-问候-深夜CN.mp3": {
        "text": "真是不可思议呢……明明都已经这么晚了，你竟然还没睡着吗？",
        "tag": "问候-深夜",
        "_id": 2546
    },
    "奥黛丽语音-问候-深夜JP.mp3": {
        "text": "アンタ、こんな時間なのに、まだ起きてるの？",
        "tag": "问候-深夜",
        "_id": 2547
    },
    "奥黛丽语音-问候-玩家生日CN.mp3": {
        "text": "如果我没有记错的话，今天是你的生日吧？既然如此，我倒也不介意陪你一起庆贺啦。",
        "tag": "玩家生日",
        "_id": 2548
    },
    "奥黛丽语音-问候-玩家生日JP.mp3": {
        "text": "私の記憶が確かなら…… 今日はアンタの誕生日よね。まあ？！ 私も忙しい身ですけど？！ アンタがどうしてもお祝いして欲しいってんなら…… 特別にお祝いしてあげてもいい、かも……。",
        "tag": "玩家生日",
        "_id": 2549
    },
    "奥黛丽语音-问候-角色生日CN.mp3": {
        "text": "今天是我的生日，如果你想陪我一起庆祝的话，最好现在就告诉我哦。不然，我可就另做安排了.....",
        "tag": "角色生日",
        "_id": 2550
    },
    "奥黛丽语音-问候-角色生日JP.mp3": {
        "text": "今日は私の誕生日よ一緒に祝いたいなら早く言いなさいよじゃないと他の予定入れちゃうから。",
        "tag": "角色生日",
        "_id": 2551
    },
    "奥黛丽语音-问候-元旦CN.mp3": {
        "text": "新一年的征程又要开始了吗？经过了之前的相处，我们似乎变得更有默契了呢……诶？等等，你可别误会了，默契什么的……我只是随口一提而已！",
        "tag": "元旦",
        "_id": 2552
    },
    "奥黛丽语音-问候-元旦JP.mp3": {
        "text": "明けましておめでとう。時間が経つのはあっという間ね。私たち出会った頃よりも気が合うようになったのかしらね？ちょ、ちょっと！ なに勘違いしてるよの！気が合うって、別にそういう意味じゃないわよ！",
        "tag": "元旦",
        "_id": 2553
    },
    "奥黛丽语音-问候-春节CN.mp3": {
        "text": "春节快乐！如果你有什么愿望，倒也可以说来听听。凭借我的能力，说不定能帮你实现呢？",
        "tag": "春节",
        "_id": 2554
    },
    "奥黛丽语音-问候-春节JP.mp3": {
        "text": "明けましておめでとう。願い事があるのなら、聞いてあげないこともないわよ。私の能力であなたの願いを叶えられるかもしれないしね。",
        "tag": "春节",
        "_id": 2555
    },
    "奥黛丽语音-问候-情人节CN.mp3": {
        "text": "情人节？你不会是为了拿巧克力，才特意跑到我这里来的吧？",
        "tag": "情人节",
        "_id": 2556
    },
    "奥黛丽语音-问候-情人节JP.mp3": {
        "text": "え？ もしかしてアンタ、チョコもらうためにわざわざ私のことろに来たの？ほんっと、バカねえ。",
        "tag": "情人节",
        "_id": 2557
    },
    "奥黛丽语音-问候-圣诞节CN.mp3": {
        "text": "唔……虽然我对圣诞节的庆祝活动没什么兴趣，但是……如果你坚持要我陪你的话，我倒也不是不可以答应你啦。",
        "tag": "圣诞节",
        "_id": 2558
    },
    "奥黛丽语音-问候-圣诞节JP.mp3": {
        "text": "うっ…… クリスマスのお祝いってほんとに興味ないんだけど、でも、どうしても一緒にお祝いしたいってんなら、付き合って上げてもいいわよ。",
        "tag": "圣诞节",
        "_id": 2559
    },
    "奥黛丽语音-问候-卡拉彼丘建立日CN.mp3": {
        "text": "卡拉彼丘纪念日？对我而言，这大概是一个意义非凡的日子吧。毕竟，它给了我一个重新开始的机会，不是吗？",
        "tag": "卡拉彼丘纪念日",
        "_id": 2560
    },
    "奥黛丽语音-问候-卡拉彼丘建立日JP.mp3": {
        "text": "カラビヤウの記念日か。きっと私にとって特別な日なんでしょうね。私はもう一度チャンスを手にしたんだから。",
        "tag": "卡拉彼丘纪念日",
        "_id": 2561
    },
    "奥黛丽语音-朋友生日CN.mp3": {
        "text": "今天是玛德蕾娜的生日，你不会不记得了吧？你说礼物？我当然有为她提前准备好礼物啊，这不是理所当然的事情吗?",
        "tag": "朋友生日",
        "_id": 2562
    },
    "奥黛丽语音-朋友生日JP.mp3": {
        "text": "今日はマダレーナの誕生日！ もちろんプレゼントも用意してあるわ。私があの娘の誕生日を忘れるわけないでしょ？",
        "tag": "朋友生日",
        "_id": 2563
    },
    "奥黛丽语音-朋友生日2CN.mp3": {
        "text": "你倒是提醒了我，原来今天是绯莎的生日啊。出于礼貌，我是不是需要为他准备一份礼物？",
        "tag": "朋友生日",
        "_id": 2564
    },
    "奥黛丽语音-朋友生日2JP.mp3": {
        "text": "今日はフューシャの誕生日よ。特別仲がいいわけじゃないけど、プレゼントは贈るわよ。礼儀として当たり前でしょ。",
        "tag": "朋友生日",
        "_id": 2565
    },
    "奥黛丽语音-018CN.mp3": {
        "text": "我叫奥黛丽·格罗夫，认识我将会是你的荣幸。",
        "tag": "获得角色",
        "_id": 2566
    },
    "奥黛丽语音-018JP.mp3": {
        "text": "はじめまして 私はオードリー・グローブ私と会えるなんて あなたとっても幸運よ。",
        "tag": "获得角色",
        "_id": 2567
    },
    "奥黛丽语音-019CN.mp3": {
        "text": "赶紧开始吧，我的时间可是很宝贵的。",
        "tag": "查看角色",
        "_id": 2568
    },
    "奥黛丽语音-019JP.mp3": {
        "text": "早く始めましょう!私の時間は貴重なんだからね!",
        "tag": "查看角色",
        "_id": 2569
    },
    "奥黛丽语音-020CN.mp3": {
        "text": "你的品位，还算不错。",
        "tag": "装配副武器",
        "_id": 2570
    },
    "奥黛丽语音-020JP.mp3": {
        "text": "アンタのセンス、悪くないわ。",
        "tag": "装配副武器",
        "_id": 2571
    },
    "奥黛丽语音-021CN.mp3": {
        "text": "决定好了吗？",
        "tag": "装配战术道具",
        "_id": 2572
    },
    "奥黛丽语音-021JP.mp3": {
        "text": "決まった？",
        "tag": "装配战术道具",
        "_id": 2573
    },
    "奥黛丽语音-023CN.mp3": {
        "text": "我等你很久了。诶？你这是什么表情嘛？你……你可千万不要误会！我只是一时兴起，绝对没有别的意思哦！(誓约10级后宿舍概率触发)",
        "tag": "打招呼",
        "_id": 2574
    },
    "奥黛丽语音-023JP.mp3": {
        "text": "待ってたわ。え？ な、なんなのよその顔！勘違いしないで！ ただ待ってたって言っただけよ！",
        "tag": "打招呼",
        "_id": 2575
    },
    "奥黛丽语音-024CN.mp3": {
        "text": "首先，我可不是那种会轻易感动的人哦！但这一次，我还算是挺开心的……所以……谢谢你啦。(誓约10级后赠送礼物概率触发)",
        "tag": "赠送角色礼物",
        "_id": 2576
    },
    "奥黛丽语音-024JP.mp3": {
        "text": "言っとくけど、私は簡単に感動するような人間ではないの。でも、今回ばかりはなんだか嬉しいわ。だから、ありがとう。",
        "tag": "赠送角色礼物",
        "_id": 2577
    },
    "奥黛丽语音-025CN.mp3": {
        "text": "对了，有件事情忘了和你说。没有经过我的允许，你可不准打探我的过去哦！否则，我可不知道会发生什么……(好感度提升后点击角色互动概率触发)",
        "tag": "互动交谈",
        "_id": 2578
    },
    "奥黛丽语音-025JP.mp3": {
        "text": "言い忘れてたけど、許可なく私の過去を選択することはやめておきなさい。もし勝手なことをしたら、どうなるかわからないわよ。",
        "tag": "互动交谈",
        "_id": 2579
    },
    "奥黛丽语音-026CN.mp3": {
        "text": "家族一直以来的经验告诉我，只有保持思维的前瞻性，才能在时代的浪潮中屹立不倒。(好感度提升后点击角色互动概率触发)",
        "tag": "互动交谈",
        "_id": 2580
    },
    "奥黛丽语音-026JP.mp3": {
        "text": "時代の潮流に立ち向かうためには、常に先を見据えることが大切。ウチの家訓よ!",
        "tag": "互动交谈",
        "_id": 2581
    },
    "奥黛丽语音-027CN.mp3": {
        "text": "如今我们也算是同伴了，但只是这样还不够哦！毕竟，只有经得起考验的情感，才不会随着时间流逝而改变。(好感度提升后点击角色互动概率触发)",
        "tag": "互动交谈",
        "_id": 2582
    },
    "奥黛丽语音-027JP.mp3": {
        "text": "あんたと仲間になれたこと、正直感謝しているわ。でも、まだまだよ。これからも一緒に試練を乗り越えましょう。私たちの絆を、揺るぎないものにするためにね。",
        "tag": "互动交谈",
        "_id": 2583
    },
    "奥黛丽语音-028CN.mp3": {
        "text": "你很难想象格罗夫家族也经历过内部纷争吧？但是，我作为亲历者，很清楚它的每一次命运变迁。(好感度提升后点击角色互动概率触发)",
        "tag": "互动交谈",
        "_id": 2584
    },
    "奥黛丽语音-028JP.mp3": {
        "text": "グローブ機が内輪揉めをしていたとは想像もつかないでしょ?私は運命の変化をすべて経験しているわ。",
        "tag": "互动交谈",
        "_id": 2585
    },
    "奥黛丽语音-029CN.mp3": {
        "text": "一切的时机都刚刚好，从而让我们一起见证了珍贵之物的出现。在你获得了它之后，必将会创造更多关于它的故事吧？(好感度提升后点击角色互动概率触发)",
        "tag": "互动交谈",
        "_id": 2586
    },
    "奥黛丽语音-029JP.mp3": {
        "text": "一緒に貴重な瞬間に立ち会うことができたわねあんたがそれを手に入れたらまた物語が生まれるんじゃないかしら。",
        "tag": "互动交谈",
        "_id": 2587
    },
    "奥黛丽语音-030CN.mp3": {
        "text": "我想这种程度的胜利恐怕还满足不了你吧？要不我们再赢一次？(战斗胜利返回宿舍触发)",
        "tag": "战斗胜利",
        "_id": 2588
    },
    "奥黛丽语音-030JP.mp3": {
        "text": "この程度の勝利では物足りないでしょ?もう一度勝ちましょう!",
        "tag": "战斗胜利",
        "_id": 2589
    },
    "奥黛丽语音-031CN.mp3": {
        "text": "你居然……这么厉害的嘛？咳咳，我的意思是……我算是认可你的实力了。这难道不值得你开心吗？(战斗胜利获得MVP返回宿舍触发)",
        "tag": "战斗胜利MVP",
        "_id": 2590
    },
    "奥黛丽语音-031JP.mp3": {
        "text": "ま、まさか、こんなに強いなんて…ああ、つまり、あんたの実力を認めたってことよ嬉しいでしょ?",
        "tag": "战斗胜利MVP",
        "_id": 2591
    },
    "奥黛丽语音-032CN.mp3": {
        "text": "怎么办？我完全不擅长安慰别人……你——过来选一种安慰方式吧！你是想我拍拍你的肩膀，还是摸摸你的头？(战斗失败返回宿舍触发)",
        "tag": "战斗失败",
        "_id": 2592
    },
    "奥黛丽语音-032JP.mp3": {
        "text": "どうしよう、人を慰めるのは本当に苦手なのよね。ここに来て、慰め方を一つ自分で選びなさい!肩を叩くか、頭なでなでか、どっちがいいのよ?",
        "tag": "战斗失败",
        "_id": 2593
    },
    "奥黛丽语音-033CN.mp3": {
        "text": "我知道你也很不甘心啦……所以，之后的战斗，你要和我一起赢回来！(战斗失败获得SVP返回宿舍触发)",
        "tag": "战斗失败SVP",
        "_id": 2594
    },
    "奥黛丽语音-033JP.mp3": {
        "text": "悔しいのはわかるわ。だけど、だからこそ、次は一緒に取り返しましょう。",
        "tag": "战斗失败SVP",
        "_id": 2595
    },
    "奥黛丽语音-034CN.mp3": {
        "text": "你不会真以为我忘记今天是你的生日了吧？我可是特意列好了计划，准备带你出去挑选生日礼物的！你这个笨蛋！(需誓约5级及以上触发)",
        "tag": "玩家生日",
        "_id": 2596
    },
    "奥黛丽语音-034JP.mp3": {
        "text": "まさか、あんたの誕生日を忘れたとでも思ってるの?今日は特別に誕生日プレゼントを選びに行くつもりだったんだからねこの馬鹿。",
        "tag": "玩家生日",
        "_id": 2597
    },
    "奥黛丽语音-035CN.mp3": {
        "text": "格罗夫家族需要的不是一个傲气凌厉的大小姐，而是一个真正睿智的领袖。也不知道如今的我，有没有回应大家的期望呢？(需誓约1级触发)",
        "tag": "交谈",
        "_id": 2598
    },
    "奥黛丽语音-035JP.mp3": {
        "text": "グローブ家に必要なのは、傲慢で威圧的なお嬢様ではなく、真に懸命なリーダーなの。今の私は、みんなの期待に応えられているのかしら?",
        "tag": "交谈",
        "_id": 2599
    },
    "奥黛丽语音-036CN.mp3": {
        "text": "我曾不慎丢失过记忆，对周围的一切都感到陌生……但正是因为这一次遭遇，我才明白最为珍贵的东西既非荣耀，也不是名利，而是纯粹的情感。(需誓约3级触发)",
        "tag": "交谈",
        "_id": 2600
    },
    "奥黛丽语音-036JP.mp3": {
        "text": "私は記憶を失って、周りのものすべてが人事のように感じたことがあったわ。でもそれがあったから、一番大事なのは、栄光でも名声でもなく、純粋な感情であることに気づいたのよ。",
        "tag": "交谈",
        "_id": 2601
    },
    "奥黛丽语音-037CN.mp3": {
        "text": "我并不否认背叛依旧有可能存在，但如果是你的话，我应该可以敞开心扉去相信吧……哼~我看你也不敢辜负我的信任嘛！(需誓约7级触发)",
        "tag": "交谈",
        "_id": 2602
    },
    "奥黛丽语音-037JP.mp3": {
        "text": "裏切りがないとは言い切れない でもあんただったら素直に信じられるはずよ まああんたも私の信頼を裏切るようなことはできないだろうしね。",
        "tag": "交谈",
        "_id": 2603
    },
    "奥黛丽语音-038CN.mp3": {
        "text": "为什么我感到疲惫的时候，会希望你能理解我的处境呢？或许，只是因为我对你多了那么一点依赖吧。(需誓约10级触发)",
        "tag": "交谈",
        "_id": 2604
    },
    "奥黛丽语音-038JP.mp3": {
        "text": "疲れている時ほど、あんたに私の立場を理解して欲しいと思うのは、なぜだろう?もしかしたら、私がそれだけあんたを頼りにしているってことなのかもね。",
        "tag": "交谈",
        "_id": 2605
    },
    "奥黛丽语音-039CN.mp3": {
        "text": "我……才没有偷偷在听摇滚乐呢！真是的，你来之前怎么不提前说一声……",
        "tag": "打招呼",
        "_id": 2606
    },
    "奥黛丽语音-039JP.mp3": {
        "text": "こっ、こっそりロックなんか聞いていないわよ!もー、どうして来る前に行ってくれないのよ。",
        "tag": "打招呼",
        "_id": 2607
    },
    "奥黛丽语音-040CN.mp3": {
        "text": "你要喝红茶吗？这可是我亲手调制的哦。当然，如果你并没有想喝的意思，我也是无所谓的啦……哼！",
        "tag": "打招呼",
        "_id": 2608
    },
    "奥黛丽语音-040JP.mp3": {
        "text": "ん?あんたも飲む?これ、私の手作りの紅茶なのよなーに?嫌とは言わないわよね。",
        "tag": "打招呼",
        "_id": 2609
    },
    "奥黛丽语音-041CN.mp3": {
        "text": "你要试试这盒巧克力吗？它的外观和口感姑且算是达到了我的要求。",
        "tag": "打招呼",
        "_id": 2610
    },
    "奥黛丽语音-041JP.mp3": {
        "text": "ふーん、このチョコレート、私の理想を満たしているわ。見た目も食感も完璧!あんたも一口どう?",
        "tag": "打招呼",
        "_id": 2611
    },
    "奥黛丽语音-042CN.mp3": {
        "text": "你看，这是今年的卡丘富豪排行榜哦。总有一天，我会让格罗夫这个姓氏出现在它的第一位。",
        "tag": "打招呼",
        "_id": 2612
    },
    "奥黛丽语音-042JP.mp3": {
        "text": "ん?来てたの?見て、これは今年のカラビヤウ不合ランキングよ。いつの日かグローブの名をこのリストの1位にしてみせるわ。",
        "tag": "打招呼",
        "_id": 2613
    },
    "奥黛丽语音-043CN.mp3": {
        "text": "拿好哦，这是我送给你的礼物。如果我什么都不做的话，那肯定就算不上是感谢了……况且，我还是有那么一点想要夸奖你的。",
        "tag": "打招呼",
        "_id": 2614
    },
    "奥黛丽语音-043JP.mp3": {
        "text": "受け取りなさい これは私からの贈り物よ何もしなかったらお礼にならないし ちょっとだけあんたを褒めたい気持ちもあるしね。",
        "tag": "打招呼",
        "_id": 2615
    },
    "奥黛丽语音-044CN.mp3": {
        "text": "果然，有时候听歌也要换一换风格的嘛。(宿舍超过3分钟无操作触发)",
        "tag": "待机",
        "_id": 2616
    },
    "奥黛丽语音-044JP.mp3": {
        "text": "たまには違った曲を聴くのも悪くないわね。",
        "tag": "待机",
        "_id": 2617
    },
    "奥黛丽语音-045CN.mp3": {
        "text": "花瓣、果脯，配合红茶本身的醇厚，真是一次完美的拼配啊。(宿舍超过3分钟无操作触发)",
        "tag": "待机",
        "_id": 2618
    },
    "奥黛丽语音-045JP.mp3": {
        "text": "花びら、ドライフルーツ、そして、紅茶本来の方純な味わいと香りのハーモニー完璧な組み合わせね!",
        "tag": "待机",
        "_id": 2619
    },
    "奥黛丽语音-046CN.mp3": {
        "text": "一般人应该不会像我这样认真地品尝巧克力吧？毕竟在这个方面，我也称得上是专家了。(宿舍超过3分钟无操作触发)",
        "tag": "待机",
        "_id": 2620
    },
    "奥黛丽语音-046JP.mp3": {
        "text": "ま、一般人じゃチョコレートの本当に味わい方はわからないでしょうね。その点私はエキスパートと言って差し支えないのですから。",
        "tag": "待机",
        "_id": 2621
    },
    "奥黛丽语音-047CN.mp3": {
        "text": "乌财社今日特报，本季度的巴布洛期货交割已顺利完成……看来，差不多该进行下一轮交易了。(宿舍超过3分钟无操作触发)",
        "tag": "待机",
        "_id": 2622
    },
    "奥黛丽语音-047JP.mp3": {
        "text": "今日の特報は…… ふんふん、今期の先物取引が順調に完了したと……。なるほど、次の取引の準備を始めないと……。",
        "tag": "待机",
        "_id": 2623
    },
    "奥黛丽语音-048CN.mp3": {
        "text": "现在我可算是有时间来陪你了，诶？你——不会是已经睡着了吧？(宿舍超过3分钟无操作触发)",
        "tag": "待机",
        "_id": 2624
    },
    "奥黛丽语音-048JP.mp3": {
        "text": "やっと時間が空いて、あんたに付き合えるわ。え?あんた、もしかしてもう寝てる?",
        "tag": "待机",
        "_id": 2625
    },
    "奥黛丽语音-049CN.mp3": {
        "text": "不要随便切换歌曲啦！都说了不想听摇滚乐了，快给我换回古典乐！",
        "tag": "打断角色状态",
        "_id": 2626
    },
    "奥黛丽语音-049JP.mp3": {
        "text": "ちょっと、勝手に曲を変えないでよ!ロックは聴きたくないって言ったでしょ?クラシックに戻して!",
        "tag": "打断角色状态",
        "_id": 2627
    },
    "奥黛丽语音-050CN.mp3": {
        "text": "诶？你在做什么呀？茶都快被弄洒了啦！",
        "tag": "打断角色状态",
        "_id": 2628
    },
    "奥黛丽语音-050JP.mp3": {
        "text": "え?何してるの?お茶がこぼれるじゃない!",
        "tag": "打断角色状态",
        "_id": 2629
    },
    "奥黛丽语音-051CN.mp3": {
        "text": "想吃这种巧克力？看来你的品味还不错嘛。",
        "tag": "打断角色状态",
        "_id": 2630
    },
    "奥黛丽语音-051JP.mp3": {
        "text": "チョコレートが欲しいの?いいセンスしてるじゃない。",
        "tag": "打断角色状态",
        "_id": 2631
    },
    "奥黛丽语音-052CN.mp3": {
        "text": "你也会对投资感兴趣吗？好吧，让我来帮你分析金融趋势，倒也不是不可以啦……",
        "tag": "打断角色状态",
        "_id": 2632
    },
    "奥黛丽语音-052JP.mp3": {
        "text": "へえ？ アンタも投資に興味あるんだ。あんまり気軽に考えないほうがいいわよ。ま、手伝ってあげてもいいけど……。",
        "tag": "打断角色状态",
        "_id": 2633
    },
    "奥黛丽语音-053CN.mp3": {
        "text": "我可不是每次都会放下手中的事情来回应你的哦。",
        "tag": "打断角色状态",
        "_id": 2634
    },
    "奥黛丽语音-053JP.mp3": {
        "text": "まったく、いつもあんたにかまってばっかりいられないのよ。勘違いしないで、今回は特別よ、特別!",
        "tag": "打断角色状态",
        "_id": 2635
    },
    "奥黛丽语音-054CN.mp3": {
        "text": "怎么？你还不明白我的意思吗？快过来和我听歌啦！",
        "tag": "近景交谈",
        "_id": 2636
    },
    "奥黛丽语音-054JP.mp3": {
        "text": "何?まだわからないの?こっちに来て、一緒に歌を聴きましょうよ！",
        "tag": "近景交谈",
        "_id": 2637
    },
    "奥黛丽语音-055CN.mp3": {
        "text": "我可不是刻意过来找你的，只是想知道你究竟在忙些什么啦……",
        "tag": "近景交谈",
        "_id": 2638
    },
    "奥黛丽语音-055JP.mp3": {
        "text": "わざわざあんたに会いに来たんじゃないからねただ、何をしているのかちょっと気になっただけよ……",
        "tag": "近景交谈",
        "_id": 2639
    },
    "奥黛丽语音-056CN.mp3": {
        "text": "多谢你了。别这样看着我嘛，这种应该感谢你的时候，我也是能坦率说出来的啦！",
        "tag": "近景交谈",
        "_id": 2640
    },
    "奥黛丽语音-056JP.mp3": {
        "text": "ありがとうそんな顔して見ないでよ、お礼ぐらいちゃんと言えるわ！",
        "tag": "近景交谈",
        "_id": 2641
    },
    "奥黛丽语音-057CN.mp3": {
        "text": "既然你都这么诚心地表达出对我的仰慕了，那让我来接受你的礼物，倒也不是不可以啦……哼，你可不要想太多哦！",
        "tag": "近景交谈",
        "_id": 2642
    },
    "奥黛丽语音-057JP.mp3": {
        "text": "そこまで真摯に私への憧れを語ってくれたのだからプレゼント特別に受け取ってあげてもいいわ深い意味なんかないんだからね。",
        "tag": "近景交谈",
        "_id": 2643
    },
    "奥黛丽语音-058CN.mp3": {
        "text": "我记得，手工巧克力的制作难度似乎并不大……等等，我这么说，可不是想亲手做给你吃哦！",
        "tag": "近景交谈",
        "_id": 2644
    },
    "奥黛丽语音-058JP.mp3": {
        "text": "手作りチョコってそんなに難しくなさそうな…ちょ、ちょっと!あんたに作ってあげたいから言ってんじゃないんだからね!",
        "tag": "近景交谈",
        "_id": 2645
    },
    "奥黛丽语音-059CN.mp3": {
        "text": "今天的歌曲品鉴到此为止了，下次可要让我听听你的歌单哦。",
        "tag": "互动交谈",
        "_id": 2646
    },
    "奥黛丽语音-059JP.mp3": {
        "text": "今日の楽曲観賞会はここまで今度はあんたのおすすめリストも聞かせてね。",
        "tag": "互动交谈",
        "_id": 2647
    },
    "奥黛丽语音-060CN.mp3": {
        "text": "据说喜欢喝同一种茶的人，连偏好也会比较相似呢。",
        "tag": "互动交谈",
        "_id": 2648
    },
    "奥黛丽语音-060JP.mp3": {
        "text": "お茶の趣味が合う人は、好みも似ているとか聞いたことがあるわ。",
        "tag": "互动交谈",
        "_id": 2649
    },
    "奥黛丽语音-061CN.mp3": {
        "text": "都是因为你总在我眼前晃来晃去的，我……我都没法忽视你的存在了！",
        "tag": "互动交谈",
        "_id": 2650
    },
    "奥黛丽语音-061JP.mp3": {
        "text": "あ、あんたがいつも目の前をうろうろしてるから、気になっちゃうんじゃないの?",
        "tag": "互动交谈",
        "_id": 2651
    },
    "奥黛丽语音-062CN.mp3": {
        "text": "我待在这里，只是为了更专注地做自己的事情，才不是为了你呢！",
        "tag": "互动交谈",
        "_id": 2652
    },
    "奥黛丽语音-062JP.mp3": {
        "text": "私は自分のことにもっと集中するためにここにいるだけあんたのためなんかじゃないからね。",
        "tag": "互动交谈",
        "_id": 2653
    },
    "奥黛丽语音-063CN.mp3": {
        "text": "你知道我名字背后的意义吗？格罗夫家族赋予了我往日的荣耀，也令我肩负起未来的责任。对大多数人来说，这或许是沉重的负担，但对我而言……它更像是机会。",
        "tag": "互动交谈",
        "_id": 2654
    },
    "奥黛丽语音-063JP.mp3": {
        "text": "私の名前の意味、知ってる?南治の名は栄光、グローブ家の未来よ、責任を託す普通の人には銃石でしょうけど、私にはチャンスのようなものよ。",
        "tag": "互动交谈",
        "_id": 2655
    },
    "奥黛丽语音-064CN.mp3": {
        "text": "呼——最近我是不是压力太大了？感觉有些疲惫了呢……",
        "tag": "特殊交谈",
        "_id": 2656
    },
    "奥黛丽语音-064JP.mp3": {
        "text": "ふう……疲れが残ってる。最近、根を詰めすぎかな……。",
        "tag": "特殊交谈",
        "_id": 2657
    },
    "奥黛丽语音-065CN.mp3": {
        "text": "如果今天见不到想见的人，应该会很烦恼吧？但是你肯定遇不到这种情况啦，所以，快对我心怀感激吧！",
        "tag": "七夕",
        "_id": 2658
    },
    "奥黛丽语音-065JP.mp3": {
        "text": "会いたい人に会えなかったりしたら困るわよねでも、あんたはきっとそんなことにはならないわ私に感謝しなさい！",
        "tag": "七夕",
        "_id": 2659
    },
    "奥黛丽语音-066CN.mp3": {
        "text": "战场上的谈判，不就是靠子弹嘛！",
        "tag": "选择角色",
        "_id": 2660
    },
    "奥黛丽语音-066JP.mp3": {
        "text": "問答無用 船長では弾丸で語りなさい！",
        "tag": "选择角色",
        "_id": 2661
    },
    "奥黛丽语音-067CN.mp3": {
        "text": "你的委托，我收到了。",
        "tag": "确认选择",
        "_id": 2662
    },
    "奥黛丽语音-067JP.mp3": {
        "text": "あなたの依頼、たしかに受け取ったわ。",
        "tag": "确认选择",
        "_id": 2663
    },
    "奥黛丽语音-068CN.mp3": {
        "text": "怎么，没见过我盛装出席吗？",
        "tag": "开场台词",
        "_id": 2664
    },
    "奥黛丽语音-068JP.mp3": {
        "text": "華麗なる登場。",
        "tag": "开场台词",
        "_id": 2665
    },
    "奥黛丽语音-069CN.mp3": {
        "text": "战斗也是要保持格调的！",
        "tag": "开场台词",
        "_id": 2666
    },
    "奥黛丽语音-069JP.mp3": {
        "text": "戦闘も優雅に！",
        "tag": "开场台词",
        "_id": 2667
    },
    "奥黛丽语音-070CN.mp3": {
        "text": "既然来了,那就取得荣耀吧！",
        "tag": "开场台词",
        "_id": 2668
    },
    "奥黛丽语音-070JP.mp3": {
        "text": "この私がいるんだから勝利を握って当然よ！",
        "tag": "开场台词",
        "_id": 2669
    },
    "奥黛丽语音-071CN.mp3": {
        "text": "准备迎接枪林弹雨吧！",
        "tag": "技能冷却完毕",
        "_id": 2670
    },
    "奥黛丽语音-071JP.mp3": {
        "text": "弾幕を浴びせる準備中よ！",
        "tag": "技能冷却完毕",
        "_id": 2671
    },
    "奥黛丽语音-072CN.mp3": {
        "text": "你可逃不出我的扫射范围。",
        "tag": "技能释放",
        "_id": 2672
    },
    "奥黛丽语音-072JP.mp3": {
        "text": "私の掃射からは逃げられないわよ。",
        "tag": "技能释放",
        "_id": 2673
    },
    "奥黛丽语音-073CN.mp3": {
        "text": "尝尝子弹的味道！",
        "tag": "技能释放",
        "_id": 2674
    },
    "奥黛丽语音-073JP.mp3": {
        "text": "私の強さを思い知らせてあげる！",
        "tag": "技能释放",
        "_id": 2675
    },
    "奥黛丽语音-074CN.mp3": {
        "text": "给我闪开！",
        "tag": "技能释放",
        "_id": 2676
    },
    "奥黛丽语音-074JP.mp3": {
        "text": "邪魔よ！",
        "tag": "技能释放",
        "_id": 2677
    },
    "奥黛丽语音-075CN.mp3": {
        "text": "盾牌已激活！",
        "tag": "被动技能生效",
        "_id": 2678
    },
    "奥黛丽语音-075JP.mp3": {
        "text": "シールドアクティブ！",
        "tag": "被动技能生效",
        "_id": 2679
    },
    "奥黛丽语音-076CN.mp3": {
        "text": "准备好了，火力全开吧。",
        "tag": "绝招冷却完毕",
        "_id": 2680
    },
    "奥黛丽语音-076JP.mp3": {
        "text": "準備完了、全力全開よ！",
        "tag": "绝招冷却完毕",
        "_id": 2681
    },
    "奥黛丽语音-077CN.mp3": {
        "text": "听好了，此路不通！",
        "tag": "绝招释放",
        "_id": 2682
    },
    "奥黛丽语音-077JP.mp3": {
        "text": "ここは通さないわ!",
        "tag": "绝招释放",
        "_id": 2683
    },
    "奥黛丽语音-078CN.mp3": {
        "text": "极致盛放！",
        "tag": "绝招释放",
        "_id": 2684
    },
    "奥黛丽语音-078JP.mp3": {
        "text": "燃え盛る!",
        "tag": "绝招释放",
        "_id": 2685
    },
    "奥黛丽语音-079CN.mp3": {
        "text": "别想逃出这片火海！",
        "tag": "绝招释放",
        "_id": 2686
    },
    "奥黛丽语音-079JP.mp3": {
        "text": "火力最大!焼き尽くしてあげる!",
        "tag": "绝招释放",
        "_id": 2687
    },
    "奥黛丽语音-080CN.mp3": {
        "text": "稍等，还不到时候。",
        "tag": "绝招冷却中",
        "_id": 2688
    },
    "奥黛丽语音-080JP.mp3": {
        "text": "待って、まだよ。",
        "tag": "绝招冷却中",
        "_id": 2689
    },
    "奥黛丽语音-109CN.mp3": {
        "text": "没什么难度嘛。",
        "tag": "击杀",
        "_id": 2690
    },
    "奥黛丽语音-109JP.mp3": {
        "text": "大したことないのね。",
        "tag": "击杀",
        "_id": 2691
    },
    "奥黛丽语音-110CN.mp3": {
        "text": "不过如此。",
        "tag": "击杀",
        "_id": 2692
    },
    "奥黛丽语音-110JP.mp3": {
        "text": "それだけ？",
        "tag": "击杀",
        "_id": 2693
    },
    "奥黛丽语音-111CN.mp3": {
        "text": "子弹的味道怎么样？",
        "tag": "击杀",
