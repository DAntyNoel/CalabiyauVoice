{
    "玛德蕾娜语音-022CN.mp3": {
        "text": "卡拉彼丘。",
        "tag": "卡拉彼丘",
        "_id": 3032
    },
    "玛德蕾娜语音-022JP.mp3": {
        "text": "カラビヤウ。",
        "tag": "卡拉彼丘",
        "_id": 3033
    },
    "玛德蕾娜语音-001CN.mp3": {
        "text": "我的卡丘身还真是纤细啊。",
        "tag": "默认交谈",
        "_id": 3034
    },
    "玛德蕾娜语音-001JP.mp3": {
        "text": "このアバターの足、すっごく細いよねっ。",
        "tag": "默认交谈",
        "_id": 3035
    },
    "玛德蕾娜语音-002CN.mp3": {
        "text": "诶？诶？你要做什么？",
        "tag": "默认交谈",
        "_id": 3036
    },
    "玛德蕾娜语音-002JP.mp3": {
        "text": "えっ？　えっ？　なになになに？",
        "tag": "默认交谈",
        "_id": 3037
    },
    "玛德蕾娜语音-003CN.mp3": {
        "text": "你是说，我的丝带歪了么？",
        "tag": "默认交谈",
        "_id": 3038
    },
    "玛德蕾娜语音-003JP.mp3": {
        "text": "ん？　リボン曲がってた？什么？",
        "tag": "默认交谈",
        "_id": 3039
    },
    "玛德蕾娜语音-004CN.mp3": {
        "text": "诶嘿嘿~被表扬了呀！",
        "tag": "默认交谈",
        "_id": 3040
    },
    "玛德蕾娜语音-004JP.mp3": {
        "text": "えへへ～！　褒められちゃった！",
        "tag": "默认交谈",
        "_id": 3041
    },
    "玛德蕾娜语音-005CN.mp3": {
        "text": "这里，不可以碰的啦……",
        "tag": "默认交谈",
        "_id": 3042
    },
    "玛德蕾娜语音-005JP.mp3": {
        "text": "ダメだよ、気安く触っちゃ・・・・・・不，我摸了摸它...",
        "tag": "默认交谈",
        "_id": 3043
    },
    "玛德蕾娜语音-006CN.mp3": {
        "text": "你是想给朋友送礼物么？不如让我来帮你绘制一份吧。",
        "tag": "朋友生日",
        "_id": 3044
    },
    "玛德蕾娜语音-006JP.mp3": {
        "text": "友達にプレゼント？あたしが描いてあげよっか？",
        "tag": "朋友生日",
        "_id": 3045
    },
    "玛德蕾娜语音-007CN.mp3": {
        "text": "诶？今天是……是我朋友的生日呀！我们一同庆祝一下吧！",
        "tag": "朋友生日",
        "_id": 3046
    },
    "玛德蕾娜语音-007JP.mp3": {
        "text": "へ～！　友達の誕生日なんだ。お祝いしてあげなよ！",
        "tag": "朋友生日",
        "_id": 3047
    },
    "玛德蕾娜语音-008CN.mp3": {
        "text": "早上好！一起描绘出美好的今天吧。",
        "tag": "问候-早",
        "_id": 3048
    },
    "玛德蕾娜语音-008JP.mp3": {
        "text": "おはよっ！今日もいい一日だよっ。",
        "tag": "问候-早",
        "_id": 3049
    },
    "玛德蕾娜语音-009CN.mp3": {
        "text": "晚上好！今夜的风景正如画作那般浪漫，你觉得呢？",
        "tag": "问候-晚",
        "_id": 3050
    },
    "玛德蕾娜语音-009JP.mp3": {
        "text": "こんばんはっ！夜の景色も絵になるよねっ。",
        "tag": "问候-晚",
        "_id": 3051
    },
    "玛德蕾娜语音-010CN.mp3": {
        "text": "莫非你也会烦心于深夜么？",
        "tag": "问候-深夜",
        "_id": 3052
    },
    "玛德蕾娜语音-010JP.mp3": {
        "text": "も～ちょっとだけ夜ふかししちゃおっか？",
        "tag": "问候-深夜",
        "_id": 3053
    },
    "玛德蕾娜语音-011CN.mp3": {
        "text": "生日快乐！希望你能在卡拉彼丘世界过得开心。今后，也请多指教了。",
        "tag": "玩家生日",
        "_id": 3054
    },
    "玛德蕾娜语音-011JP.mp3": {
        "text": "お誕生日おめでとー！君のカラビヤウライフが一層たのしい日々になりますようにっ！これからもよろしくねっ！",
        "tag": "玩家生日",
        "_id": 3055
    },
    "玛德蕾娜语音-012CN.mp3": {
        "text": "你竟然记得我的生日！谢谢！我真是太高兴了！",
        "tag": "角色生日",
        "_id": 3056
    },
    "玛德蕾娜语音-012JP.mp3": {
        "text": "あたしの誕生日、覚えてくれてたんだねっ！ありがとう！　とっても嬉しいよ！",
        "tag": "角色生日",
        "_id": 3057
    },
    "玛德蕾娜语音-013CN.mp3": {
        "text": "新年快乐！我今年的愿望，是让更多的人喜欢我的画作。如果你也能喜欢的话，那就更好了。",
        "tag": "元旦",
        "_id": 3058
    },
    "玛德蕾娜语音-013JP.mp3": {
        "text": "明けましておめでと～！今年はもっとたくさんの人にあたしのデザイン楽しんでもらえるといいな～。",
        "tag": "元旦",
        "_id": 3059
    },
    "玛德蕾娜语音-014CN.mp3": {
        "text": "春节快乐！每年的这个时候，我都会满心期待，因为我真的很喜欢热闹的爆竹声！",
        "tag": "春节",
        "_id": 3060
    },
    "玛德蕾娜语音-014JP.mp3": {
        "text": "春節おめでとう！あたしお祭り大好き！　どっかで爆竹やってないかなっ！",
        "tag": "春节",
        "_id": 3061
    },
    "玛德蕾娜语音-015CN.mp3": {
        "text": "圣诞快乐！我今年设计了好多圣诞系列的服装哦。一起去街上看看有没有人穿吧！",
        "tag": "圣诞节",
        "_id": 3062
    },
    "玛德蕾娜语音-015JP.mp3": {
        "text": "メリークリスマス！今年はサンタ衣装、たくさん描かせてもらえたんだ～。着てくれてる人いるか見に行こうよ！",
        "tag": "圣诞节",
        "_id": 3063
    },
    "玛德蕾娜语音-016CN.mp3": {
        "text": "这个，给你！这是情人节巧克力。是商家特别推荐给我的哟！",
        "tag": "情人节",
        "_id": 3064
    },
    "玛德蕾娜语音-016JP.mp3": {
        "text": "はいこれ、あたしからっ！バレンタインのチョコレートだよっ。この店のやつ、おすすめなんだー。",
        "tag": "情人节",
        "_id": 3065
    },
    "玛德蕾娜语音-017CN.mp3": {
        "text": "今天是卡拉彼丘纪念日！真希望越来越多的人能喜欢上我设计的衣服呀！",
        "tag": "卡拉彼丘纪念日",
        "_id": 3066
    },
    "玛德蕾娜语音-017JP.mp3": {
        "text": "今日はカラビヤウの入国記念日だよっ！もっともーっとあたしの作った服を楽しんでもらいたいな！",
        "tag": "卡拉彼丘纪念日",
        "_id": 3067
    },
    "玛德蕾娜语音-018CN.mp3": {
        "text": "很高兴见到你！我的名字叫玛德蕾娜，是姐姐的专属时装绘师！请多关照啦。",
        "tag": "获得角色",
        "_id": 3068
    },
    "玛德蕾娜语音-018JP.mp3": {
        "text": "初めましてっ！あたしはマダレーナ・リッリ。お姉ちゃん専属のデザイナーなんだ！これからよろしくねっ！",
        "tag": "获得角色",
        "_id": 3069
    },
    "玛德蕾娜语音-019CN.mp3": {
        "text": "嗯，准备战斗了！",
        "tag": "查看角色",
        "_id": 3070
    },
    "玛德蕾娜语音-019JP.mp3": {
        "text": "バトルの準備だねっ！",
        "tag": "查看角色",
        "_id": 3071
    },
    "玛德蕾娜语音-020CN.mp3": {
        "text": "选择这个武器就可以了吗？",
        "tag": "装配副武器",
        "_id": 3072
    },
    "玛德蕾娜语音-020JP.mp3": {
        "text": "この武器でいいの？",
        "tag": "装配副武器",
        "_id": 3073
    },
    "玛德蕾娜语音-021CN.mp3": {
        "text": "决定好了嘛？",
        "tag": "装配战术道具",
        "_id": 3074
    },
    "玛德蕾娜语音-021JP.mp3": {
        "text": "装備決まった？",
        "tag": "装配战术道具",
        "_id": 3075
    },
    "玛德蕾娜语音-023CN.mp3": {
        "text": "哟！你最近怎么样？我可是状态良好哦！",
        "tag": "打招呼",
        "_id": 3076
    },
    "玛德蕾娜语音-023JP.mp3": {
        "text": "やあ！　調子はどうかな？あたしはばっちしだよっ！",
        "tag": "打招呼",
        "_id": 3077
    },
    "玛德蕾娜语音-024CN.mp3": {
        "text": "卡拉彼丘是一个神奇的世界，有那么多有趣的事情让我沉迷其中。我曾以为绘画就是我的一切，可是有一天我意识到，你才是那个最让我在意的人！接下来的日子里，我能一直注视着你吗？",
        "tag": "赠送角色礼物",
        "_id": 3078
    },
    "玛德蕾娜语音-024JP.mp3": {
        "text": "カラビヤウには好奇心をくすぐる素敵なものがたくさんあるんだ。でも気がついた。一番気になるのは君なんだって！　君のそばでずっと君を見つめさせてね。",
        "tag": "赠送角色礼物",
        "_id": 3079
    },
    "玛德蕾娜语音-025CN.mp3": {
        "text": "嗯……这样，是不是有些不太好啊……",
        "tag": "互动交谈",
        "_id": 3080
    },
    "玛德蕾娜语音-025JP.mp3": {
        "text": "さ、触り方がいやらしいよ・・・・・・",
        "tag": "互动交谈",
        "_id": 3081
    },
    "玛德蕾娜语音-026CN.mp3": {
        "text": "唔……难道你对我的卡丘身感兴趣吗？",
        "tag": "互动交谈",
        "_id": 3082
    },
    "玛德蕾娜语音-026JP.mp3": {
        "text": "んんっ・・・・・・あたしのアバターに興味あるの、かな？",
        "tag": "互动交谈",
        "_id": 3083
    },
    "玛德蕾娜语音-027CN.mp3": {
        "text": "啊……好难为情哟，不要再看我了。",
        "tag": "互动交谈",
        "_id": 3084
    },
    "玛德蕾娜语音-027JP.mp3": {
        "text": "は、恥ずかしいよ。あんまり見ないで・・・・・・",
        "tag": "互动交谈",
        "_id": 3085
    },
    "玛德蕾娜语音-028CN.mp3": {
        "text": "欸嘿——你的手，感觉好温柔。",
        "tag": "互动交谈",
        "_id": 3086
    },
    "玛德蕾娜语音-028JP.mp3": {
        "text": "ふふ、君の手、とっても気持ちがいいね。",
        "tag": "互动交谈",
        "_id": 3087
    },
    "玛德蕾娜语音-029CN.mp3": {
        "text": "唔……别，别这样……都发出奇怪的声音了……",
        "tag": "互动交谈",
        "_id": 3088
    },
    "玛德蕾娜语音-029JP.mp3": {
        "text": "あ、んっ・・・・・・やだ・・・・・・　変な声出ちゃったじゃないか・・・・・・",
        "tag": "互动交谈",
        "_id": 3089
    },
    "玛德蕾娜语音-030CN.mp3": {
        "text": "真是太好啦！我证明了自己的战斗实力呢！",
        "tag": "战斗胜利MVP",
        "_id": 3090
    },
    "玛德蕾娜语音-030JP.mp3": {
        "text": "やったやった！バトルだってできちゃうとこ、見せちゃったねっ！",
        "tag": "战斗胜利MVP",
        "_id": 3091
    },
    "玛德蕾娜语音-031CN.mp3": {
        "text": "好厉害啊！今天状态真的很好哟！继续加油哇~！",
        "tag": "战斗胜利MVP",
        "_id": 3092
    },
    "玛德蕾娜语音-031JP.mp3": {
        "text": "すごーい！　もう絶好調って感じだよ～！この調子でがんばろうねっ！",
        "tag": "战斗胜利MVP",
        "_id": 3093
    },
    "玛德蕾娜语音-032CN.mp3": {
        "text": "唔……又输了呀……",
        "tag": "战斗失败",
        "_id": 3094
    },
    "玛德蕾娜语音-032JP.mp3": {
        "text": "うええ～！　また負けちゃったよ～！",
        "tag": "战斗失败",
        "_id": 3095
    },
    "玛德蕾娜语音-033CN.mp3": {
        "text": "没事的没事的，人总会有运气不好的时候！所以，不要伤心，好吗？",
        "tag": "战斗失败",
        "_id": 3096
    },
    "玛德蕾娜语音-033JP.mp3": {
        "text": "だいじょぶだいじょぶ！　こんな日もあるって！だから落ち込まないで。ね？",
        "tag": "战斗失败",
        "_id": 3097
    },
    "玛德蕾娜语音-034CN.mp3": {
        "text": "生日快乐！我特意画了一幅画，送给你作为生日礼物！",
        "tag": "玩家生日",
        "_id": 3098
    },
    "玛德蕾娜语音-034JP.mp3": {
        "text": "お誕生日おめでとう～！お祝いのイラストを描き下ろしたよっ！あたしから君へのプレゼントっ！",
        "tag": "玩家生日",
        "_id": 3099
    },
    "玛德蕾娜语音-035CN.mp3": {
        "text": "你是想和我一起画画吗？那当然非常欢迎呀！毕竟我们是朋友对吧？和朋友一起，无论做什么，我都很开心！",
        "tag": "交谈",
        "_id": 3100
    },
    "玛德蕾娜语音-035JP.mp3": {
        "text": "私と一緒に遊びたいの？　いいよっ！友達になれたら、ずーっと一緒にいてあげるっ！",
        "tag": "交谈",
        "_id": 3101
    },
    "玛德蕾娜语音-036CN.mp3": {
        "text": "你是帮我把素描本放到六号架子的第三层上了吗？那真是多谢了！作为回礼，我为你画一幅画怎么样？",
        "tag": "交谈",
        "_id": 3102
    },
    "玛德蕾娜语音-036JP.mp3": {
        "text": "このスケッチブックは三段目の六番の棚に置いて・・・・・・　よしオッケー！片付けしてくれてありがとねっ！　君は休憩しててよ。その間、君の似顔絵描いててあげるっ！",
        "tag": "交谈",
        "_id": 3103
    },
    "玛德蕾娜语音-037CN.mp3": {
        "text": "最近，我好像渐渐看清了你眼中的世界，一个五彩斑斓的世界，一个高洁纯净的世界。那么，你知道，我眼中的世界是什么样子吗？",
        "tag": "交谈",
        "_id": 3104
    },
    "玛德蕾娜语音-037JP.mp3": {
        "text": "最近、君の世界がよく見える気がするんだ。カラフルな世界、素敵で高貴な世界・・・・・・ね？　君にも私の世界が見える？",
        "tag": "交谈",
        "_id": 3105
    },
    "玛德蕾娜语音-038CN.mp3": {
        "text": "你为什么不尝试着为我画一幅画呢？在卡拉彼丘，世间的一切都能由画笔，油漆和画布来承载。如果万物都能成为艺术品，那该是一件多么美妙的事情呀！",
        "tag": "交谈",
        "_id": 3106
    },
    "玛德蕾娜语音-038JP.mp3": {
        "text": "ねえねえ、たまには私を描いてみない？カラビヤウでは全てが筆であり、絵の具であり、キャンバス！　全てが素敵なアートなんだよっ！",
        "tag": "交谈",
        "_id": 3107
    },
    "玛德蕾娜语音-039CN.mp3": {
        "text": "你看，天空。我很喜欢天空，因为云朵之上的澄澈总让能让我脑海中灵感奔涌。",
        "tag": "打招呼",
        "_id": 3108
    },
    "玛德蕾娜语音-039JP.mp3": {
        "text": "ぼーっと空を見てるの楽しいよね。いろんなインスピレーションが湧いてきちゃう。",
        "tag": "打招呼",
        "_id": 3109
    },
    "玛德蕾娜语音-040CN.mp3": {
        "text": "一直聊天会不会有点无聊呀？",
        "tag": "打招呼",
        "_id": 3110
    },
    "玛德蕾娜语音-040JP.mp3": {
        "text": "あたし、話が長いと飽きちゃうんだよね～。",
        "tag": "打招呼",
        "_id": 3111
    },
    "玛德蕾娜语音-041CN.mp3": {
        "text": "这个叫做G28射手步枪，是我最喜欢的武器哦。",
        "tag": "打招呼",
        "_id": 3112
    },
    "玛德蕾娜语音-041JP.mp3": {
        "text": "G28 E2スナイパーライフルちゃん。これがあたしの愛用武器だよっ！",
        "tag": "打招呼",
        "_id": 3113
    },
    "玛德蕾娜语音-042CN.mp3": {
        "text": "姐姐——嗯……你有看到我的姐姐吗？",
        "tag": "打招呼",
        "_id": 3114
    },
    "玛德蕾娜语音-042JP.mp3": {
        "text": "（クレメンティーナを探している）お姉ちゃ～ん！あ、ねえねえ君！　どっかでお姉ちゃん見なかった？",
        "tag": "打招呼",
        "_id": 3115
    },
    "玛德蕾娜语音-043CN.mp3": {
        "text": "我其实有想过要训练的啦，但忙到现在都还没有开始……",
        "tag": "打招呼",
        "_id": 3116
    },
    "玛德蕾娜语音-043JP.mp3": {
        "text": "バトルのトレーニングもしなきゃって、思ってはいるんだけどね～・・・・・・",
        "tag": "打招呼",
        "_id": 3117
    },
    "玛德蕾娜语音-044CN.mp3": {
        "text": "唔……终于做好了呀！那么，谁先拿到就是谁的啦！啊——呜！（卖萌）",
        "tag": "待机",
        "_id": 3118
    },
    "玛德蕾娜语音-044JP.mp3": {
        "text": "そ～っ・・・・・・っと、取れた！取った人が食べていいルールねっ！ぱくっ！",
        "tag": "待机",
        "_id": 3119
    },
    "玛德蕾娜语音-045CN.mp3": {
        "text": "嗯……每次从这里眺望天空时，脑海中都会涌现不少灵感呢。",
        "tag": "待机",
        "_id": 3120
    },
    "玛德蕾娜语音-045JP.mp3": {
        "text": "ごろごろ～・・・・・・ここから空を眺めてるとインスピレーションが湧いてくるんだよね～。",
        "tag": "待机",
        "_id": 3121
    },
    "玛德蕾娜语音-046CN.mp3": {
        "text": "啊……啊？怎么了？不好意思，刚才……刚才太入迷了。",
        "tag": "待机",
        "_id": 3122
    },
    "玛德蕾娜语音-046JP.mp3": {
        "text": "・・・・・・えっ？　なに？ごめん、ぜんぜん聞いてなかったよ。什么？",
        "tag": "待机",
        "_id": 3123
    },
    "玛德蕾娜语音-047CN.mp3": {
        "text": "唔……我真的不太擅长打扫呀，如果不在姐姐回来之前整理完的话，姐姐会对我失望的。",
        "tag": "待机",
        "_id": 3124
    },
    "玛德蕾娜语音-047JP.mp3": {
        "text": "はあ～・・・・・・　お片付けって苦手なんだよ。やらなとお姉ちゃんに怒られちゃうからね～・・・・・・",
        "tag": "待机",
        "_id": 3125
    },
    "玛德蕾娜语音-048CN.mp3": {
        "text": "嗯哼~~嗯哼……这个么？我正在准备炒菜所需要的食材。",
        "tag": "待机",
        "_id": 3126
    },
    "玛德蕾娜语音-048JP.mp3": {
        "text": "ふんふ～ふんふん・・・・・・これ？　食事に使うお野菜を切ってるんだよ。",
        "tag": "待机",
        "_id": 3127
    },
    "玛德蕾娜语音-049CN.mp3": {
        "text": "你也想吃曲奇嘛？那好，张开嘴巴——啊——",
        "tag": "打断角色状态",
        "_id": 3128
    },
    "玛德蕾娜语音-049JP.mp3": {
        "text": "君もクッキー食べる？はい、あーん。",
        "tag": "打断角色状态",
        "_id": 3129
    },
    "玛德蕾娜语音-050CN.mp3": {
        "text": "只是眺望着天空中的云，一天就这么过去了呢。",
        "tag": "打断角色状态",
        "_id": 3130
    },
    "玛德蕾娜语音-050JP.mp3": {
        "text": "お空の雲を眺めてるだけで、一日過ぎちゃうね～。",
        "tag": "打断角色状态",
        "_id": 3131
    },
    "玛德蕾娜语音-051CN.mp3": {
        "text": "没错！是这样的感觉吧……诶？你，一直都待在这里吗？",
        "tag": "打断角色状态",
        "_id": 3132
    },
    "玛德蕾娜语音-051JP.mp3": {
        "text": "よしっ、こんなもんかな・・・・・・あれ？　まだいたんだ",
        "tag": "打断角色状态",
        "_id": 3133
    },
    "玛德蕾娜语音-052CN.mp3": {
        "text": "啊，好~累啊……那些每天都能打扫房间的人，真的太了不起了哇。",
        "tag": "打断角色状态",
        "_id": 3134
    },
    "玛德蕾娜语音-052JP.mp3": {
        "text": "はあ、疲れたあ～・・・・・・毎日お片付けできる人って天才だと思うよ・・・・・・",
        "tag": "打断角色状态",
        "_id": 3135
    },
    "玛德蕾娜语音-053CN.mp3": {
        "text": "好哇！OK，我的刀工还是很不错的。",
        "tag": "打断角色状态",
        "_id": 3136
    },
    "玛德蕾娜语音-053JP.mp3": {
        "text": "よしオッケー。お野菜はいつもまとめて切ってるんだ。",
        "tag": "打断角色状态",
        "_id": 3137
    },
    "玛德蕾娜语音-054CN.mp3": {
        "text": "欸嘿——其实没什么啦。",
        "tag": "近景交谈",
        "_id": 3138
    },
    "玛德蕾娜语音-054JP.mp3": {
        "text": "えへへっ、なんでもなーいよっ。",
        "tag": "近景交谈",
        "_id": 3139
    },
    "玛德蕾娜语音-055CN.mp3": {
        "text": "我画了新的设计稿！想听一听你的建议呀。",
        "tag": "近景交谈",
        "_id": 3140
    },
    "玛德蕾娜语音-055JP.mp3": {
        "text": "新しいデザインのラフ描いたんだ。率直な感想聞かせて欲しいんだ。",
        "tag": "近景交谈",
        "_id": 3141
    },
    "玛德蕾娜语音-056CN.mp3": {
        "text": "嗯——欸嘿（笑）~~谢谢你，一直以来，都是如此。我总是想要感谢你，对我如此地照顾呢。",
        "tag": "近景交谈",
        "_id": 3142
    },
    "玛德蕾娜语音-056JP.mp3": {
        "text": "じ～・・・・・ふふっ。ううん、いつもありがとうって思って。",
        "tag": "近景交谈",
        "_id": 3143
    },
    "玛德蕾娜语音-057CN.mp3": {
        "text": "那个……你眼中的我是什么样的呢？",
        "tag": "近景交谈",
        "_id": 3144
    },
    "玛德蕾娜语音-057JP.mp3": {
        "text": "えっと・・・・・・　君ってあたしの事どう思ってるのかなぁ。",
        "tag": "近景交谈",
        "_id": 3145
    },
    "玛德蕾娜语音-058CN.mp3": {
        "text": "怎么啦？不如把心事悄悄告诉我吧？",
        "tag": "近景交谈",
        "_id": 3146
    },
    "玛德蕾娜语音-058JP.mp3": {
        "text": "どうしたの？　あたしに内緒話かなっ？",
        "tag": "近景交谈",
        "_id": 3147
    },
    "玛德蕾娜语音-059CN.mp3": {
        "text": "嘿嘿……那个，果然我完全不会收拾房间呢，不要看了啦——",
        "tag": "互动交谈",
        "_id": 3148
    },
    "玛德蕾娜语音-059JP.mp3": {
        "text": "あはは・・・・・・あたし、片付けとかぜんっぜんしないから・・・・・・部屋はあんまり見ないでね？",
        "tag": "互动交谈",
        "_id": 3149
    },
    "玛德蕾娜语音-060CN.mp3": {
        "text": "嗯……现在想来，之前的设计的确是不太行啊……看来我得再多找一找灵感，考虑更多的细节……",
        "tag": "互动交谈",
        "_id": 3150
    },
    "玛德蕾娜语音-060JP.mp3": {
        "text": "うーん・・・・・・　このデザインじゃ、ぜんぜんダメだ・・・・・・もっとインスピレーションを膨らませて、もっともっとディティールを詰めないと・・・・・・",
        "tag": "互动交谈",
        "_id": 3151
    },
    "玛德蕾娜语音-061CN.mp3": {
        "text": "嗯……不好意思呀，刚才，我正在构思下一幅画的内容呢。",
        "tag": "互动交谈",
        "_id": 3152
    },
    "玛德蕾娜语音-061JP.mp3": {
        "text": "うう～ん・・・・・・あ、ごめんね。次のデザイン考えてたら上の空になっちゃった。",
        "tag": "互动交谈",
        "_id": 3153
    },
    "玛德蕾娜语音-062CN.mp3": {
        "text": "你觉得我和姐姐的关系很好？那当然呀！我非常尊敬姐姐的时尚眼光哦！",
        "tag": "互动交谈",
        "_id": 3154
    },
    "玛德蕾娜语音-062JP.mp3": {
        "text": "お姉ちゃんと仲いいんだねって？うん！　ファッションへの想いの強さとか、すっごく尊敬してる！",
        "tag": "互动交谈",
        "_id": 3155
    },
    "玛德蕾娜语音-063CN.mp3": {
        "text": "我真的很高兴能够来到卡拉彼丘呀！",
        "tag": "互动交谈",
        "_id": 3156
    },
    "玛德蕾娜语音-063JP.mp3": {
        "text": "あたし、カラビヤウに来ることができて本当によかったって思ってるんだ",
        "tag": "互动交谈",
        "_id": 3157
    },
    "玛德蕾娜语音-064CN.mp3": {
        "text": "轻点儿，再轻点儿……啊，又被发现了！你也太敏锐了，偶尔也让我赢一次不好嘛——",
        "tag": "特殊交谈",
        "_id": 3158
    },
    "玛德蕾娜语音-064JP.mp3": {
        "text": "そーっとそーっとぉ・・・・・・　あー！　また負けちゃったあ！君って、ジェンガめちゃくちゃ強いよね。たまにはあたしに花を持たせてくれてもいいんじゃないかなぁ？",
        "tag": "特殊交谈",
        "_id": 3159
    },
    "玛德蕾娜语音-065CN.mp3": {
        "text": "以前的我总觉得，只有和姐姐在一起，我才能做到这些事情。可有一天我发现……你好像也有着和姐姐一样的魔法，和你待在一起的时候，我的脑海里便涌现出源源不断的灵感……这是为什么呢？嗯，这一定是因为你很特别吧。",
        "tag": "七夕",
        "_id": 3160
